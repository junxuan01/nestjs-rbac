{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "spec": false,
  "generateOptions": {
    "spec": false
  },
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": true,
          "introspectComments": true
        }
      }
    ]
  }
}
